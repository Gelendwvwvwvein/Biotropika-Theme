{
  "apiVersion": 2,
  "name": "biotropika/honor-board-block",
  "title": "Доска почёта",
  "category": "widgets",
  "icon": "editor-ul",
  "description": "Список людей с перетаскиваемой сортировкой",
  "supports": { "html": false, "customClassName": true },
  "attributes": {
    "term":           { "type": "integer", "default": 0 },
    "order":          { "type": "string",  "default": "manual" },
    "manualOrder":    { "type": "array",   "default": [] },
    "showLaps":       { "type": "boolean","default": true },
    "showDesc":       { "type": "boolean","default": true },
    "showRegalia":    { "type": "boolean","default": true }
  },
  "editorScript": "file:./src/index.js",
  "editorStyle":  "file:./style/editor.css",
  "script":       "file:./src/frontend.js",
  "style":        "file:./src/style/frontend.css"
}
